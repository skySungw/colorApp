<view class="my-card_container"><cu-custom vue-id="3e39b98b-1" bgColor="bg-gradual-green" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">我的贴子</view></cu-custom><scroll-view class="bg-white nav fixed" style="{{'top:'+(customBar+'px')+';'}}" scroll-x="{{true}}"><view class="flex text-center"><block wx:for="{{menu}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item','flex-1',[(index==curTab)?'text-green cur':'']]}}" data-id="{{index}}" data-event-opts="{{[['tap',[['tabSelect',['$event']]]]]}}" bindtap="__e">{{item.label}}</view></block></view></scroll-view><view><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><card vue-id="{{'3e39b98b-2-'+index}}" item="{{item}}" source="{{1}}" status="{{curTab}}" data-event-opts="{{[['^resetList',[['initParams']]]]}}" bind:resetList="__e" bind:__l="__l"></card></block></view><block wx:if="{{list.length==0}}"><empty vue-id="3e39b98b-3" msg="暂无数据~" bind:__l="__l"></empty></block></view>