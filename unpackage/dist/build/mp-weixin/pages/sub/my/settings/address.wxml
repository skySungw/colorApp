<view class="my-setting_modifyaddress"><cu-custom vue-id="6297b213-1" bgColor="bg-gradual-green" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">{{(id?'编辑':'新增')+"地址"}}</view></cu-custom><form><view class="cu-form-group margin-top"><view class="title">联系人</view><input placeholder="姓名" data-event-opts="{{[['input',[['__set_model',['$0','receiveContact','$event',[]],['params']]]]]}}" value="{{params.receiveContact}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">联系方式</view><input placeholder="联系方式" data-event-opts="{{[['input',[['__set_model',['$0','receivePhone','$event',[]],['params']]]]]}}" value="{{params.receivePhone}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">地区信息</view><input disabled="{{true}}" data-event-opts="{{[['tap',[['selectMap',['$event']]]]]}}" value="{{location}}" bindtap="__e"/><text class="cuIcon-location"></text></view><view class="cu-form-group"><view class="title">详细地址</view><input placeholder="街道门牌号信息" data-event-opts="{{[['input',[['__set_model',['$0','receiveDetailAddress','$event',[]],['params']]]]]}}" value="{{params.receiveDetailAddress}}" bindinput="__e"/></view><button class="fixed bottom" type="primary" data-event-opts="{{[['tap',[['onSave',['$event']]]]]}}" bindtap="__e">保存</button></form></view>